{
  "name": "jet-passport-backend",
  "version": "1.0.0",
  "description": "Backend for Chicago Passport & Visa Expedite",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "postinstall": "npx puppeteer browsers install chrome",
    "dev": "nodemon index.ts",
    "start": "node dist/index.js",
    "build": "tsc && copyfiles -u 1 \"templates/**/*.ejs\" dist/templates",
    "type-check": "tsc --noEmit",
    "seed:homepage": "ts-node scripts/seedHomepage.ts",
    "jp-back-deploy": "git pull origin main && pnpm install && pnpm build && pm2 restart jp-back",
    "jp-back-stage": "git pull origin dev && pnpm install && pnpm build && pm2 restart jp-back"
  },
  "author": "beingmusthafa <139311904+beingmusthafa@users.noreply.github.com>",
  "license": "ISC",
  "repository": "https://github.com/ZikrabyteSolution/secure-visa-admin-backend.git",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.627.0",
    "@aws-sdk/s3-request-presigner": "^3.627.0",
    "@types/ejs": "^3.1.5",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/node-cron": "^3.0.11",
    "@types/puppeteer": "^7.0.4",
    "aws-sdk": "npm:@aws-sdk/client-s3",
    "axios": "^1.7.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.5.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.10.0",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "sib-api-v3-sdk": "^8.5.0",
    "socket.io": "^4.8.3",
    "ts-node": "^10.9.2",
    "twilio": "^5.3.3",
    "typescript": "^5.5.3"
  },
